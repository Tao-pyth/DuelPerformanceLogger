#:kivy 2.2.1

<ConfirmDialog@MDDialog>:
    title_text: ""
    message_text: ""
    ok_text: ""
    cancel_text: ""
    ok_callback: lambda *_: None
    cancel_callback: lambda *_: None
    auto_dismiss: False
    title: root.title_text
    text: root.message_text
    buttons:
        MDFlatButton:
            text: root.cancel_text
        MDRaisedButton:
            text: root.ok_text